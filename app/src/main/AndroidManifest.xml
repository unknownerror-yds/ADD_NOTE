<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sampledatabase">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_baseline_library_books_24"
        android:label="@string/app_name_new"
        android:roundIcon="@drawable/ic_baseline_library_books_24"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleDatabase">
        <activity android:name=".Splash_Activity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity">
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>